{
  "name": "Backstage",
  "version": "0.1.0",
  "description": "Renderer and organisator of different components",
  "repository": "git@github.com:bdo-labs/backstage.git",
  "main": "lib/index.js",
  "style": "index.css",
  "scripts": {
    "start": "atomify",
    "test": "karma"
  },
  "atomify": {
    "assets": {
      "prefix": "build/assets/",
      "dest": "build/assets"
    },
    "js": {
      "entry": "lib/index.js",
      "alias": "bdo.js",
      "output": "build/bundle.js"
    },
    "css": {
      "entry": "index.css",
      "alias": "bdo.css",
      "output": "build/bundle.css"
    },
    "server": {
      "sync": true,
      "path": "index.html",
      "open": true
    }
  },
  "browserify": {
    "transform": [
      "partialify"
    ]
  },
  "devDependencies": {
    "angular": "^1.2.18",
    "angular-ui-router": "bdo-labs/ui-router#activeparent",
    "atomify": "^3.1.2",
    "browserify": "^4.2.0",
    "karma": "^0.12.22",
    "karma-browserifast": "^0.6.1",
    "karma-chrome-launcher": "^0.1.4",
    "karma-jasmine": "^0.1.5",
    "partialify": "^3.1.1"
  },
  "dependencies": {
    "user": "bdo-labs/user",
    "styles": "bdo-labs/styles",
    "strategy": "bdo-labs/strategy",
    "custom-elements": "bdo-labs/custom-elements",
    "filters": "bdo-labs/filters"
  }
}
