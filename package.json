{
  "name": "Backstage",
  "version": "0.1.0",
  "description": "Renderer and organisator of different components",
  "repository": "git@github.com:bdo-labs/backstage.git",
  "devDependencies": {
    "angular": "^1.2.18",
    "angular-ui-router": "^0.2.10",
    "atomify": "^3.1.2",
    "browserify": "^4.2.0",
    "karma": "^0.12.16",
    "karma-browserifast": "^0.6.1",
    "karma-jasmine": "^0.1.5",
    "partialify": "^3.1.1"
  },
  "dependencies": {
    "user": "git+ssh://git@github.com:bdo-labs/user.git",
    "styles": "git+ssh://git@github.com:bdo-labs/styles.git",
    "strategi": "git+ssh://git@github.com:bdo-labs/strategi.git"
  },
  "scripts": {
    "build": "atomify"
  },
  "main": "lib/index.js",
  "atomify": {
    "js": {
      "entry": "lib/index.js",
      "output": "build/bundle.js"
    },
    "css": {
      "entry": "index.css",
      "output": "build/bundle.css"
    }
  },
  "browserify": {
    "transforms": [
      "partialify"
    ]
  }
}
