{
  "name": "Backstage",
  "version": "0.1.0",
  "description": "Renderer and organisator of different components",
  "repository": "git@github.com:bdo-labs/backstage.git",
  "devDependencies": {
    "angular": "^1.2.18",
    "angular-ui-router": "git+ssh://git@github.com:bdo-labs/ui-router.git#activeparent",
    "atomify": "^3.1.2",
    "browserify": "^4.2.0",
    "karma": "^0.12.16",
    "karma-browserifast": "^0.6.1",
    "karma-jasmine": "^0.1.5",
    "partialify": "^3.1.1"
  },
  "dependencies": {
    "user": "git+ssh://git@github.com:bdo-labs/user.git",
    "styles": "git+ssh://git@github.com:bdo-labs/styles.git",
    "strategy": "git+ssh://git@github.com:bdo-labs/strategy.git",
    "custom-elements": "git+ssh://git@github.com:bdo-labs/custom-elements.git",
    "filters": "git+ssh://git@github.com:bdo-labs/filters.git"
  },
  "main": "lib/index.js",
  "atomify": {
    "js": {
      "entry": "lib/index.js",
      "output": "build/bundle.js"
    },
    "css": {
      "entry": "index.css",
      "output": "build/bundle.css",
      "assets": {
        "prefix": "assets/",
        "dest": "build/assets"
      }
    }
  },
  "browserify": {
    "transform": [
      "partialify"
    ]
  }
}
